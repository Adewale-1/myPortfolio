<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<svg width="800" height="500" xmlns="http://www.w3.org/2000/svg">
  <!-- Background -->
  <rect width="800" height="500" fill="#4a6cf7" rx="0" ry="0"/>
  
  <!-- Title -->
  <text x="400" y="80" font-family="Arial" font-size="36" text-anchor="middle" font-weight="bold" fill="#ffffff">Natural Language to SQL</text>
  
  <!-- Natural Language Input -->
  <rect x="100" y="150" width="600" height="70" fill="#e8eaf6" rx="8" ry="8" stroke="#3f51b5" stroke-width="2"/>
  <text x="400" y="193" font-family="Arial" font-size="22" text-anchor="middle" fill="#333">
    "Show all flights from New York to Boston"
  </text>
  
  <!-- Arrow -->
  <polygon points="370,240 430,240 400,280" fill="#ffffff"/>
  
  <!-- SQL Output -->
  <rect x="100" y="300" width="600" height="90" fill="#e0f7fa" rx="8" ry="8" stroke="#00bcd4" stroke-width="2"/>
  <text x="170" y="340" font-family="monospace" font-size="22" fill="#01579b">SELECT * FROM flights</text>
  <text x="170" y="375" font-family="monospace" font-size="22" fill="#01579b">WHERE origin = 'NYC' AND dest = 'BOS'</text>
  
  <!-- Text at bottom -->
  <text x="400" y="450" font-family="Arial" font-size="20" text-anchor="middle" fill="#ffffff">
    T5 Fine-tuning & In-context Learning
  </text>
</svg> 